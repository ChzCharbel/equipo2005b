<!--Busqueda profesores -->
<div class="input-group mb-3 w-50 mx-auto">
  <input
    type="text"
    class="form-control"
    placeholder="Buscar..."
    aria-label="Buscar"
  />
  <button id="demo" class="btn btn-outline-secondary" type="button">
    Buscar
  </button>
</div>
<!--Botones profesores -->
<% for (let profesor of profesores) { %>
<div class="container mt-4">
  <div class="row justify-content-center">
    <div class="col-md-6 d-flex flex-column gap-2">
      <button
        type="button"
        class="btn btn-secondary"
        data-bs-toggle="modal"
        data-bs-target="#profModal<%= profesor.matriculaProfesor %>"
        data-prof-name="<%= profesor.nombreProfesor %>"
        data-prof-id="<%= profesor.matriculaProfesor %>"
      >
        <%= profesor.nombreProfesor %>
      </button>
    </div>
  </div>
</div>

<%- include('_modal_horario', { profesor: profesor }) %> <% } %>
